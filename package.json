{
  "name": "emptying",
  "version": "0.1.0",
  "description": "Checking or pruning the empty value.",
  "repository": "https://github.com/cncolder/emptying.git",
  "license": "MIT",
  "author": "colder",
  "sideEffects": false,
  "exports": {
    "import": "./index.mjs",
    "require": "./index.js"
  },
  "main": "index.js",
  "module": "index.mjs",
  "files": [
    "index.js",
    "index.mjs"
  ],
  "scripts": {
    "build": "tsc && mv index.js index.mjs && tsc -m commonjs",
